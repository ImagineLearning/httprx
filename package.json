{
	"name": "@imaginelearning/fetchrx",
	"author": "Imagine Learning",
	"version": "0.1.0",
	"license": "MIT",
	"main": "dist/index.js",
	"module": "dist/fetchrx.esm.js",
	"typings": "dist/index.d.ts",
	"files": [
		"dist",
		"src"
	],
	"engines": {
		"node": ">=10"
	},
	"scripts": {
		"start": "tsdx watch",
		"build": "tsdx build",
		"test": "tsdx test --watch",
		"lint": "eslint \"src/**/*.{js,ts}\"",
		"format": "prettier --config .prettierrc --write \"**/*.{js,json,ts}\""
	},
	"dependencies": {
		"immer": "^7.0.5",
		"lodash": "^4.17.19",
		"lodash-es": "^4.17.15"
	},
	"peerDependencies": {
		"rxjs": "^6.6.0"
	},
	"devDependencies": {
		"@types/lodash": "^4.14.157",
		"husky": "^4.2.5",
		"jest": "^26.1.0",
		"jest-fetch-mock": "^3.0.3",
		"lint-staged": "^10.2.11",
		"node-fetch": "^2.6.0",
		"prettier": "^2.0.5",
		"pretty-quick": "^2.0.1",
		"tsdx": "^0.13.2",
		"tslib": "^2.0.0",
		"typescript": "^3.9.7"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged -p false"
		}
	},
	"prettier": {
		"printWidth": 80,
		"semi": true,
		"singleQuote": true,
		"trailingComma": "es5"
	},
	"lint-staged": {
		"*.(js|json|ts)": "pretty-quick --staged --config .prettierrc",
		"*.(js|ts)": "yarn lint"
	}
}
